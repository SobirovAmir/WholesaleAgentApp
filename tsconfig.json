{
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "node16",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "target": "esnext"
  },
  "include": [
    "src/**/*",
    "node_modules/@types/**/*"
  ],
  "extends": "expo/tsconfig.base"
}
